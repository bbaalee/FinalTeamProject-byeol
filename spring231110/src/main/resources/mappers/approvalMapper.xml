<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myweb.www.repository.ApprovalDAO">
<insert id="approval_club"  parameterType="com.myweb.www.domain.ApprovalVO">
insert into approval (emp_no,club_no) values (#{empNo},#{clubNo})

</insert>

<select id="approvalExist" resultType="long">
    SELECT emp_no FROM approval WHERE emp_no = #{empNo}
</select>

<!-- <select id="list_club" resultType="com.myweb.www.domain.ApprovalDTO">

SELECT * FROM approval
JOIN club ON approval.club_no = club.club_cd
where approval.club_no IS NOT NULL;

</select> -->

<select id="total_club" resultType="int">
select count(club_no) from approval where club_no is not null;

</select>
</mapper>