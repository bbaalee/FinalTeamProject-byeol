package com.myweb.www.domain;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
import lombok.ToString;

@Getter
@Setter
@ToString
@AllArgsConstructor
@NoArgsConstructor
public class BoardVO {
	/*
	 * CREATE TABLE `board` ( 
	 * `dep_bno` bigint(20) NOT NULL AUTO_INCREMENT, 
	 * `emp_no` bigint(20) NOT NULL, 
	 * `emp_nm` varchar(50) NOT NULL, 
	 * `title` varchar(500) NOT NULL, 
	 * `content` text NOT NULL, 
	 * `reg_date` datetime DEFAULT CURRENT_TIMESTAMP,
	 * `mod_date` datetime DEFAULT CURRENT_TIMESTAMP, 
	 * `type` varchar(20) NOT NULL,
	 * `club_cd` varchar(100) DEFAULT NULL, 
	 * `dep_cd` varchar(100) DEFAULT NULL,
	 * `cmt_qty` int(11) DEFAULT '0', 
	 * `read_qty` int(11) DEFAULT '0', 
	 * PRIMARY KEY (`dep_bno`) )
	 */
	
	private long depBno;
	private long empNo;
	private long empNm;
	private String title;
	private String content;
}
